{
  "framework": "jasmine2",
  "src_files": [
    "spec/*.ts"
  ],
  "serve_files": [
    "spec-bundle.js"
  ],
  "before_tests": "rimraf tmp && tsc -p ./spec && rollup -c rollup.config-spec.js",
  "on_exit": "rm ./spec-bundle.js && rm ./spec-bundle.js.map",
  "launch_in_dev": [ "firefox", "chrome" ]
}
