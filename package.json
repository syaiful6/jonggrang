{
  "name": "jonggrang",
  "version": "0.1.0",
  "author": {
    "name": "Syaiful Bahri"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syaiful6/jonggrang.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "es6/jonggrang.es6.js",
  "jsnext:main": "es6/jonggrang.es6.js",
  "license": "MIT",
  "dependencies": {
    "data.task": "^3.1.0",
    "flyd": "^0.2.2",
    "ramda": "^0.19.1"
  },
  "devDependencies": {
    "mkdirp": "0.5.x",
    "rollup": "^0.34.7",
    "testem": "^1.10.3",
    "ts-node": "^1.2.2",
    "tsify": "^1.0.3",
    "tslint": "^3.14.0-dev.1",
    "typescript": "^2.1.0-dev.20160815",
    "uglify-js": "^2.7.1",
    "typings": "^1.3.3",
    "@types/mocha": "^2.2.29",
    "@types/node": "^6.0.33",
    "assert": "^1.4.1",
    "browserify": "^13.1.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "typescript": "^2.1.0-dev.20160815"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf es6",
    "lint": "tslint src/*.ts src/**/*.ts",
    "prebuild:commonjs": "rimraf lib",
    "build:commonjs": "tsc",
    "prebuild:dist": "tsc -declaration --module es2015 --moduleResolution node --noFallthroughCasesInSwitch --noImplicitAny --noImplicitReturns --noUnusedLocals --outDir tmp/ --preserveConstEnums --pretty --sourceMap --strictNullChecks --target es5 src/lib/flyd.d.ts src/lib/data.task.d.ts src/index.ts",
    "build:dist": "rollup -c",
    "postbuild:dist": "rimraf tmp",
    "build": "npm run build:commonjs && npm run build:dist",
    "test:browser": "browserify test/index.ts -p tsify > tests-bundle.js && testem && rm tests-bundle.js",
    "test": "npm run lint && npm run test:browser",
    "typings": "typings"
  }
}
